<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ruihua.face.recognition">
    <!--权限注册-->
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <!--界面注册-->
    <application
        android:allowBackup="true"
        android:supportsRtl="true">
        <activity android:name=".ui.FaceGatewayActivity" />
        <activity android:name=".ui.RgbDetectActivity" />
        <activity android:name=".ui.RgbVideoIdentityActivity" />
        <activity android:name=".ui.LivenessSettingActivity" />
        <activity android:name=".ui.RegActivity" />
        <activity android:name=".ui.UserListActivity" />
        <activity android:name=".ui.UserActivity" />

    </application>
</manifest>