<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/nine_record_ll"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:gravity="center_vertical"
    android:orientation="horizontal">

    <TextView
        android:id="@+id/nine_record_one"
        style="@style/text_title_content_style"
        android:layout_width="0dp"
        android:layout_weight="2" />

    <View style="@style/view_line_style" />

    <TextView
        android:id="@+id/nine_record_two"
        style="@style/text_title_content_style"
        android:layout_width="0dp"
        android:layout_weight="3" />

    <View style="@style/view_line_style" />

    <TextView
        android:id="@+id/nine_record_three"
        style="@style/text_title_content_style"
        android:layout_width="0dp"
        android:layout_weight="4" />

    <View style="@style/view_line_style" />

    <TextView
        android:id="@+id/nine_record_four"
        style="@style/text_title_content_style"
        android:layout_width="0dp"
        android:layout_weight="2" />

    <View style="@style/view_line_style" />

    <TextView
        android:id="@+id/nine_record_five"
        style="@style/text_title_content_style"
        android:layout_width="0dp"
        android:layout_weight="2.3" />

    <View style="@style/view_line_style" />

    <TextView
        android:id="@+id/nine_record_six"
        style="@style/text_title_content_style"
        android:layout_width="0dp"
        android:layout_weight="1" />

    <View style="@style/view_line_style" />

    <TextView
        android:id="@+id/nine_record_seven1"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/x10"
        android:layout_marginRight="@dimen/x10"
        android:layout_weight="2.4"
        android:gravity="center"
        android:paddingLeft="@dimen/x10"
        android:paddingTop="@dimen/y10"
        android:paddingRight="@dimen/x10"
        android:paddingBottom="@dimen/y10"
        android:textSize="16sp" />

    <View style="@style/view_line_style" />

    <TextView
        android:id="@+id/nine_record_eight"
        style="@style/text_title_content_style"
        android:layout_width="0dp"
        android:layout_weight="2" />

    <View style="@style/view_line_style" />


    <TextView
        android:id="@+id/nine_record_nine"
        style="@style/text_title_content_style"
        android:layout_width="0dp"
        android:layout_weight="2.3" />

</LinearLayout>