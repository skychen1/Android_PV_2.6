<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout android:id="@+id/swipe"
				xmlns:android="http://schemas.android.com/apk/res/android"
				xmlns:app="http://schemas.android.com/apk/res-auto"
				android:layout_width="match_parent"
				android:layout_height="wrap_content">


	<LinearLayout
		android:id="@+id/seven_ll"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:gravity="center_vertical"
		android:orientation="horizontal">

		<View style="@style/view_line_style"/>

		<LinearLayout
			android:id="@+id/seven_one_ll"
			android:layout_width="0dp"
			android:layout_height="match_parent"
			android:layout_weight="1"
			android:gravity="center">

			<CheckBox
				android:id="@+id/seven_one"
				android:layout_width="@dimen/x35"
				android:layout_height="@dimen/x35"
				android:button="@drawable/checkbox_dialog_sel"
				android:gravity="center_horizontal"/>

		</LinearLayout>

		<View style="@style/view_line_style"/>

		<TextView
			android:id="@+id/seven_two"
			style="@style/text_title_content_style"
			android:layout_width="0dp"
			android:layout_weight="1.5"/>

		<View style="@style/view_line_style"/>

		<TextView
			android:id="@+id/seven_three"
			style="@style/text_title_content_style"
			android:layout_width="0dp"
			android:layout_weight="1.5"/>


		<View style="@style/view_line_style"/>

		<TextView
			android:id="@+id/seven_four"
			style="@style/text_title_content_style"
			android:layout_width="0dp"
			android:layout_weight="1.5"/>

		<View style="@style/view_line_style"/>

		<TextView
			android:id="@+id/seven_five"
			style="@style/text_title_content_style"
			android:layout_width="0dp"
			android:layout_weight="2"/>

		<View style="@style/view_line_style"/>

		<TextView
			android:id="@+id/seven_six"
			style="@style/text_title_content_style"
			android:layout_width="0dp"
			android:layout_weight="0.8"/>

		<View style="@style/view_line_style"/>

		<TextView
			android:id="@+id/seven_seven"
			style="@style/text_title_content_style"
			android:layout_width="0dp"
			android:layout_weight="0.8"/>

		<View style="@style/view_line_style"/>

		<TextView
			android:id="@+id/seven_eight"
			style="@style/text_title_content_style"
			android:layout_width="0dp"
			android:layout_weight="2.5"/>

		<View style="@style/view_line_style"/>
		<TextView
			android:id="@+id/seven_nine"
			style="@style/text_title_content_style"
			android:layout_width="0dp"
			android:layout_weight="1"/>

		<View style="@style/view_line_style"/>
		<TextView
			android:id="@+id/seven_ten"
			style="@style/text_title_content_style"
			android:layout_width="0dp"
			android:layout_weight="4"/>

		<View style="@style/view_line_style"/>
	</LinearLayout>
</RelativeLayout>