<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
			  xmlns:app="http://schemas.android.com/apk/res-auto"
			  android:orientation="vertical"
			  android:layout_width="@dimen/x800"
			  android:layout_height="wrap_content"
			  android:gravity="center_horizontal"
			  android:background="@drawable/dialog_bg"
				android:paddingLeft="@dimen/x80"
				android:paddingRight="@dimen/x80"
				android:paddingBottom="@dimen/y60"
				android:paddingTop="@dimen/y50"
			  android:layout_gravity="center_horizontal"
	>
	<ImageView
		android:id="@+id/dialog_closs"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_alignParentRight="true"
		android:src="@mipmap/hccz_ic_tc_gb"
		/>
	<TextView
		android:id="@+id/storeroom_title"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_alignParentLeft="true"
		android:text="请选择库房"
		android:gravity="center"
		android:textSize="@dimen/textsize_21"
		android:textStyle="bold"
		android:textColor="@color/text_color_3"/>
	<GridView
		android:id="@+id/storeroom_view"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_marginTop="@dimen/x40"
		android:gravity="center_horizontal"
		android:layout_marginBottom="@dimen/x40"
		android:verticalSpacing="@dimen/x35"
		android:layout_centerInParent="true"
		android:overScrollMode="never"
		android:background="@color/bg_f"
		android:paddingTop="@dimen/x3"
		android:paddingBottom="@dimen/x3"
		android:layout_below="@+id/storeroom_title"
		android:numColumns="3" />

	<TextView
		android:id="@+id/dialog_sure"
		android:layout_width="@dimen/x400"
		android:layout_height="@dimen/x80"
		android:layout_below="@+id/storeroom_view"
		android:background="@drawable/bg_btn_gray_sel2"
		android:layout_centerHorizontal="true"
		android:text="确定"
		android:gravity="center"
		android:textColor="@color/bg_f"
		android:textSize="@dimen/textsize_18"
		/>
</RelativeLayout>