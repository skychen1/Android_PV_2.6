<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/nine_ll"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:gravity="center_vertical"
    android:orientation="horizontal">
    <View
        style="@style/view_line_style"/>
    <LinearLayout
        android:layout_width="0dp"
        android:layout_weight="1"
        android:layout_height="match_parent"
        android:gravity="center">
        <CheckBox
            android:id="@+id/nine_zero"
            android:layout_height="@dimen/x35"
            android:enabled="false"
            android:gravity="center_horizontal"
            android:button="@drawable/checkbox_dialog_sel"
            android:layout_width="@dimen/x35"
            />

    </LinearLayout>
    <View style="@style/view_line_style" />
    <TextView
        android:id="@+id/nine_one"
        style="@style/text_title_content_style"
        android:layout_width="0dp"
        android:layout_weight="1.6"
        android:paddingLeft="0dp"
        android:paddingRight="0dp"/>

    <View style="@style/view_line_style" />

    <TextView
        android:id="@+id/nine_two"
        style="@style/text_title_content_style"
        android:layout_width="0dp"
        android:layout_weight="3" />

    <View style="@style/view_line_style" />

    <TextView
        android:id="@+id/nine_three"
        style="@style/text_title_content_style"
        android:layout_width="0dp"
        android:layout_weight="4"
        android:paddingRight="0dp"
        android:paddingLeft="0dp"/>

    <View style="@style/view_line_style" />

    <TextView
        android:id="@+id/nine_four"
        style="@style/text_title_content_style"
        android:layout_width="0dp"
        android:layout_weight="2" />

    <View style="@style/view_line_style" />

    <TextView
        android:id="@+id/nine_five"
        style="@style/text_title_content_style"
        android:layout_width="0dp"
        android:layout_weight="2"
        android:paddingRight="0dp"
        android:paddingLeft="0dp"/>

    <View style="@style/view_line_style" />

    <TextView
        android:id="@+id/nine_six"
        style="@style/text_title_content_style"
        android:layout_width="0dp"
        android:layout_weight="1.5" />

    <View style="@style/view_line_style" />

    <TextView
        android:id="@+id/nine_seven1"
        style="@style/text_title_content_style"
        android:layout_width="0dp"
        android:layout_weight="2" />

    <View style="@style/view_line_style" />

    <TextView
        android:id="@+id/nine_eight"
        style="@style/text_title_content_style"
        android:layout_width="0dp"
        android:layout_weight="2"
        android:paddingRight="0dp"
        android:paddingLeft="0dp"/>

    <View style="@style/view_line_style" />


    <TextView
        android:id="@+id/nine_nine1"
        style="@style/text_title_content_style"
        android:layout_width="0dp"
        android:layout_weight="1.6"
        android:paddingRight="0dp"
        android:paddingLeft="0dp"/>
    <View style="@style/view_line_style" />
</LinearLayout>