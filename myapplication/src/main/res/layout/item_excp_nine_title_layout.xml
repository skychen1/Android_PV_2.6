<?xml version="1.0" encoding="utf-8"?>
<LinearLayout android:id="@+id/nine_ll"
			  xmlns:android="http://schemas.android.com/apk/res/android"
			  android:layout_width="match_parent"
			  android:layout_height="wrap_content"
			  android:orientation="horizontal">

	<TextView
		android:id="@+id/nine_one"
		style="@style/text_title_style"
		android:layout_width="0dp"
		android:layout_weight="2.4"
		android:text="异常来源"
		/>

	<View
		style="@style/view_title_line_style"/>

	<TextView
		android:id="@+id/nine_two"
		style="@style/text_title_style"
		android:layout_width="0dp"
		android:text="耗材名称"
		android:layout_weight="3"
		/>

	<View
		style="@style/view_title_line_style"/>

	<TextView
		android:id="@+id/nine_three"
		style="@style/text_title_style"
		android:layout_width="0dp"
		android:text="EPC"
		android:layout_weight="4"
		/>

	<View
		style="@style/view_title_line_style"/>

	<TextView
		android:id="@+id/nine_four"
		android:text="型号规格"
		style="@style/text_title_style"
		android:layout_width="0dp"
		android:layout_weight="2"
		/>

	<View
		style="@style/view_title_line_style"/>

	<TextView
		android:text="效期"
		android:id="@+id/nine_five"
		style="@style/text_title_style"
		android:layout_width="0dp"
		android:layout_weight="2.3"
		/>

	<View
		style="@style/view_title_line_style"/>

	<TextView
		android:text="柜号"
		android:id="@+id/nine_six"
		style="@style/text_title_style"
		android:layout_width="0dp"
		android:layout_weight="1"
		/>

	<View
		style="@style/view_title_line_style"/>

	<TextView
		android:text="操作人"
		android:id="@+id/nine_seven"
		style="@style/text_title_style"
		android:layout_width="0dp"
		android:layout_weight="2"
		/>

	<View
		style="@style/view_title_line_style"/>

	<TextView
		android:text="操作时间"
		android:id="@+id/nine_eight"
		style="@style/text_title_style"
		android:layout_width="0dp"
		android:layout_weight="2.3"
		/>

	<View
		style="@style/view_title_line_style"/>

	<TextView
		android:text="操作"
		android:id="@+id/nine_nine"
		style="@style/text_title_style"
		android:layout_width="0dp"
		android:layout_weight="2"
		/>


</LinearLayout>