        <activity android:name="com.ruihua.libfacerecognitionv3.main.activity.FaceUserGroupListActivity"></activity>
        <activity android:name="com.ruihua.libfacerecognitionv3.main.activity.FaceUserInfoActivity"></activity>
        <activity android:name="com.ruihua.libfacerecognitionv3.main.activity.FaceUserListActivity"></activity>
        <activity android:name="com.ruihua.libfacerecognitionv3.main.activity.FaceRGBCloseDebugSearchActivity"></activity>
    </application>
</manifest>